{
  "name": "sovtech-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.4",
    "jest-cli": "^27.0.5",
    "jest-styled-components": "^7.0.4",
    "luxon": "^1.27.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.1.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "styled-theme": "^0.3.3",
    "styled-tools": "^1.7.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:watch": "yarn jest --watch",
    "test": "yarn jest",
    "testCoverageReport": "yarn jest --coverage --coverageDirectory=output/coverage/jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/private/jest/cssAsStringMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/setupTests.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}
